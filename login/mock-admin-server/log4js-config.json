{
    "appenders": [
		{
			"type": "dateFile",
			"filename": "./log/",
			"pattern": "yyyy-MM-dd.log",
			"alwaysIncludePattern": true,
            "layout": {
                "type": "pattern",
                "pattern": "prod"

            }
		},
		{
            "type": "console",
            "layout": {
                "type": "pattern",
                "pattern": "%d{yyyy-MM-dd hh:mm:ss, SSS} %p %z  %m"

            }
		}
	],
    "levels":{
        "[all]": "DEBUG"
    }
}
